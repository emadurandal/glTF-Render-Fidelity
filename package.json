{
  "name": "rfw",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "cd ./glTF-Sample-Renderer && npm install && npm run build",
    "postinstall": "cp ./glTF-Sample-Renderer/dist/libs/*.wasm ./glTF-Sample-Renderer/dist/ && cp ./glTF-Sample-Renderer/dist/libs/* ./public/libs/",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "images": "cp -r 'glTF-Render-Fidelity-Generator/test/goldens/.' 'public/images/goldens'",
    "thumbnail": "node src/scripts/create_thumbnails.mjs",
    "metadata": "node src/scripts/extract_model_metadata.mjs"
  },
  "dependencies": {
    "@babylonjs/core": "^8.14.0",
    "@babylonjs/loaders": "^8.14.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/model-viewer": "^4.1.0",
    "@khronosgroup/gltf-viewer": "./glTF-Sample-Renderer",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/material-nextjs": "^6.3.1",
    "fuse.js": "^7.0.0",
    "gl-matrix": "^3.4.3",
    "image-js": "^0.36.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "three": "^0.172.0",
    "three-gpu-pathtracer": "^0.0.23"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/showdown": "^2.0.6",
    "@types/three": "^0.178.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "showdown": "^2.1.0",
    "typescript": "^5"
  }
}
